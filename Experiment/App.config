<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="mssqllocaldb" />
      </parameters>
    </defaultConnectionFactory>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
  <appSettings>
    <add key="NotificationServerUrl" value="http://192.168.1.120:1405/" />
    <add key="SpeechSynthesizer_Volume" value="100" />
    <add key="SpeechSynthesizer_Rate" value="0" />
    <add key="SpeechSynthesizer_VoiceGender" value="Female" />
    <add key="SpeechSynthesizer_VoiceAge" value="Child" />
    
    <!--Barcode printers-->
    <add key="TTLabelPrinterName" value="TSC TTP-247" />
    <add key="MUBLabelPrinterName" value="TSC TTP-247 (Copy 1)" />
    <add key="ReceiptPrinterName" value="TSC TTP-247" />

    <!--Smart card reader/writer-->
    <add key="SmartCardContactlessReaderName" value="ACS ACR122 0" />

    <!--Smart card printer-->
    <add key="SmartCardPrinterContactlessReaderName" value="Identive SDI010 Contactless Reader 0" />
    <add key="SmartCardPrinterContactReaderName" value="Identive SDI010 Smart Card Reader 0" />
    <add key="SmartCardPrinterSerialNumber" value="Z9J171200014" />
    <add key="SmartCardPrinterName" value="Zebra ZXP Series 9 USB Card Printer" />

    <!--Debug environment-->
    <add key="No_SmartCardContactlessReader" value="true" />
    <add key="No_SmartCardPrinter" value="true" />
    <add key="No_BarcodePrinter" value="true" />

    <!--others-->
    <add key="ClientSettingsProvider.ServiceUri" value="" />
    
    
    <add key="IsLocal" value="true" />
    <add key="COMPort" value="COM5" />
    <add key="BaudRate" value="2400" />
    <add key="Parity" value="None" />
  </appSettings>
  <connectionStrings>
    <add name="SSKEntities" connectionString="metadata=res://*/DBContext.SSK.csdl|res://*/DBContext.SSK.ssdl|res://*/DBContext.SSK.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=192.168.1.120;initial catalog=SSK;persist security info=True;user id=ssk;password=123456;multipleactiveresultsets=True;application name=EntityFramework&quot;" providerName="System.Data.EntityClient" />
    <add name="TrinityCentralizedDBEntities" connectionString="metadata=res://*/DBContext.TrinityCentralizedDB.csdl|res://*/DBContext.TrinityCentralizedDB.ssdl|res://*/DBContext.TrinityCentralizedDB.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=192.168.1.120;initial catalog=TrinityCentralizedDB;persist security info=True;user id=ssk;password=123456;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
  </connectionStrings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-10.0.0.0" newVersion="10.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
