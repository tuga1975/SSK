<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="mssqllocaldb"/>
      </parameters>
    </defaultConnectionFactory>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer"/>
    </providers>
  </entityFramework>
  <appSettings>
    <!--Barcode printers-->
    <add key="TTLabelPrinterName" value="TSC TTP-244 Pro"/>
    <add key="MUBLabelPrinterName" value="TSC TTP-244 Pro"/>
    <add key="ReceiptPrinterName" value="TSC TTP-244 Pro"/>

    <!--Smart card reader/writer-->
    <add key="SmartCardContactlessReaderName" value="ACS ACR122 0"/>

    <!--Smart card printer-->
    <add key="SmartCardPrinterContactlessReaderName" value="Identive SDI010 Contactless Reader 0"/>
    <add key="SmartCardPrinterContactReaderName" value="Identive SDI010 Smart Card Reader 0"/>
    <add key="SmartCardPrinterSerialNumber" value="Z9J171200014"/>

    <!--Debug environment-->
    <add key="No_SmartCardContactlessReader" value="true"/>
    <add key="No_SmartCardPrinter" value="true"/>
    <add key="No_BarcodePrinter" value="true"/>

    <!--others-->
    <add key="ClientSettingsProvider.ServiceUri" value=""/>
  </appSettings>
  <connectionStrings>
    <add name="SSKEntities" connectionString="metadata=res://*/DBContext.SSK.csdl|res://*/DBContext.SSK.ssdl|res://*/DBContext.SSK.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=(local);initial catalog=SSK;persist security info=True;user id=ssk;password=123456;multipleactiveresultsets=True;application name=EntityFramework&quot;" providerName="System.Data.EntityClient"/>
    <add name="TrinityCentralizedDBEntities" connectionString="metadata=res://*/DBContext.TrinityCentralizedDB.csdl|res://*/DBContext.TrinityCentralizedDB.ssdl|res://*/DBContext.TrinityCentralizedDB.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=(local);initial catalog=TrinityCentralizedDB;persist security info=True;user id=ssk;password=123456;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient"/>
  </connectionStrings>
</configuration>
